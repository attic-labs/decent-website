{
  "name": "decentdb-landing-page",
  "version": "0.1.0",
  "description": "Landing page for decentdb",
  "main": "src/index.js",
  "scripts": {
    "start": "serve ./build",
    "dev": "webpack-dev-server",
    "clean": "rm -rf build",
    "precommit": "lint-staged",
    "prerelease": "npm run clean && NODE_ENV=production webpack",
    "release": "now",
    "test": "xo"
  },
  "lint-staged": {
    "linters": {
      "*.{js}": [
        "npm run test",
        "prettier --single-quote --no-semi --trailing-comma all --bracket-spacing false --print-width 100 --write",
        "git add"
      ]
    }
  },
  "xo": {
    "extends": ["prettier"],
    "env": ["browser", "node"],
    "rules": {
      "no-warning-comments": 1
    }
  },
  "author": "Jesse Ditson <jesse@attic.io>",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.24.1",
    "eslint-config-prettier": "2.3.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "now": "^8.2.5",
    "prettier": "1.5.3",
    "svg-react-loader": "^0.4.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.6.1",
    "xo": "0.18.2"
  },
  "dependencies": {
    "serve": "^6.0.6"
  }
}
